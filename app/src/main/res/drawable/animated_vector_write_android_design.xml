<!-- The animated vector drawable that will do the writing  -->

<animated-vector

    xmlns:android="http://schemas.android.com/apk/res/android"

    android:drawable="@drawable/android_design"

    >

    <target

        android:name="andro"

        android:animation="@anim/write_andro"

        />

    <target

        android:name="id"

        android:animation="@anim/write_id"

        />

    <target

        android:name="a_stroke"

        android:animation="@anim/write_a_stroke"

        />

    <target

        android:name="android_i_dot"

        android:animation="@anim/write_android_i_dot"

        />

    <target

        android:name="d"

        android:animation="@anim/write_d"

        />

    <target

        android:name="esig"

        android:animation="@anim/write_esig"

        />

    <target

        android:name="n"

        android:animation="@anim/write_n"

        />

    <target

        android:name="design_i_dot"

        android:animation="@anim/write_design_i_dot"

        />

</animated-vector>