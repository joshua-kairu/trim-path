    xmlns:android="http://schemas.android.com/apk/res/android"

    >

    <objectAnimator

        android:propertyName="trimPathEnd"

        android:valueFrom="1"

        android:valueTo="0"

        android:startOffset="1320"

        android:duration="50"

        android:interpolator="@android:interpolator/accelerate_decelerate"

        />

